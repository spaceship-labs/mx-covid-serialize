{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "no-await-in-loop": "off",
    "no-console": "off",
    "import/no-named-as-default": "off",
    "import/no-named-as-default-member": "off",
    "jsx-a11y/no-static-element-interactions": "off",
    "jsx-quotes": [
      "error",
      "prefer-single"
    ],
    "eol-last": [
      "error",
      "always"
    ]
  },
  "env": {
    "browser": false,
    "node": true
  },
  "globals": {
    "document": false
  }
}